{
  "name": "youtube-grave-robber",
  "version": "1.0.0",
  "private": true,
  "workspaces": ["client", "server"],
  "type": "module",
  "scripts": {
    "dev": "sh -c 'npm -w server run dev & npm -w client run dev; wait'",
    "dev:server": "npm -w server run dev",
    "dev:client": "npm -w client run dev",
    "test": "npm run -ws test",
    "test:server": "npm -w server run test",
    "test:client": "npm -w client run test"
  }
}
